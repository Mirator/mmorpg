generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Player {
  id           String   @id
  state        Json
  stateVersion Int      @default(1)
  createdAt    DateTime @default(now())
  updatedAt    DateTime @default(now()) @updatedAt
  lastSeenAt   DateTime?
}
